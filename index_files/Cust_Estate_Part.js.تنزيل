$('.EstateQ').change(function(){var v2=$(this).val();var url='/Parts/GetPart';$.get(url,{id:v2},function(data){$(".PartQ").empty();var v="<option>اختر الشقة</option>";$.each(data,function(index,v1){v+="<option value='"+v1.Id+"'>"+v1.Name+"</option>";});$(".PartQ").html(v);$('.PartQ').change();});});$('.PartQ').change(function(){var v2=$(this).val();var url='/Custs/GetCust';$.get(url,{id:v2},function(data){$(".CustQ").empty();var v="<option>اختر المستأجر</option>";$.each(data,function(index,v1){v+="<option value='"+v1.Id+"'>"+v1.Name+"</option>";});$(".CustQ").html(v);});if($('.NoteW').length)
{var url2='/Parts/GetPart_Cont';$.get(url2,{id:v2},function(data){$(".NoteW").val(data.Note);$(".MonyW").val(data.Mony);});}});$('.Estate_Null').change(function(){var v2=$(this).val();var url='/Parts/GetPart_Null';$.get(url,{id:v2},function(data){$(".PartQ").empty();var v="<option>اختر الشقة</option>";$.each(data,function(index,v1){v+="<option value='"+v1.Id+"'>"+v1.Name+"</option>";});$(".PartQ").html(v);$('.PartQ').change();});});$('.Estate_Full').change(function(){var v2=$(this).val();var url='/Parts/GetPart_Full';$.get(url,{id:v2},function(data){$(".PartQ").empty();var v="<option>اختر الشقة</option>";$.each(data,function(index,v1){v+="<option value='"+v1.Id+"'>"+v1.Name+"</option>";});$(".PartQ").html(v);$('.PartQ').change();});});